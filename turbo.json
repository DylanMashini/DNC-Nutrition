{
	"$schema": "https://turbo.build/schema.json",
	"pipeline": {
		"build": {
			"outputs": [".next/**"],
			"inputs": ["*", ":!:.vscode/*", ":!:cypress/*", ":!:.github/*"]
		},
		"test-build": {
			"outputs": [".next/**"],
			"inputs": ["*", ":!:.vscode/*", ":!:cypress/*", ":!:.github/*"]
		},
		"cypress": {
			"dependsOn": ["test-build"],
			"inputs": ["cypress/*", "cypress.json", ".next/*"]
		},
		"lint": {
			"outputs": []
		}
	}
}
